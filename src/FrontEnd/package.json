{
  "name": "root",
  "private": true,
  "scripts": {
    "@comment build": "Optionally pass a blueprint name. IE npm run build example",
    "build": "npm-run-all \"build:server {1}\" \"build:client {1}\" --",
    "build:server": "npm-run-all \"node-build:server -- --env.BLUEPRINT={1}\" --",
    "node-build:server": "node node_modules/webpack/bin/webpack.js --config=config/webpack/prodServer.js",
    "build:client": "npm-run-all \"node-build:client -- --env.BLUEPRINT={1}\" --",
    "node-build:client": "node node_modules/webpack/bin/webpack.js --config=config/webpack/prodClient.js",
    "build:docker": "node node_modules/webpack/bin/webpack.js --config=config/webpack/prod.js",
    "create-blueprint": "node ./scripts/createBlueprint",
    "eslint": "eslint . --ext .ts,.tsx",
    "eslint:fix": "npm run eslint -- --fix",
    "eslint:blueprint": "eslint ./modules/blueprints --ext .ts,.tsx -c ./.eslintrc-blueprint.js --no-eslintrc",
    "eslint:blueprint:fix": "npm run eslint:blueprint -- --fix",
    "lint:css": "ECHO use npm run stylelint",
    "prettier": "prettier --parser typescript ./{config,modules,scripts,tools}/**/*.{ts,tsx,js} --write",
    "start": "node startDevelopment.js",
    "@comment start": "Optionally pass a blueprint name and port. IE npm run start example 8080",
    "setupTsconfigPaths": "node ./config/webpack/setupTsconfigPathsFile.js",
    "start:example": "node startDevelopment.js example 4000",
    "start:prod": "CLS && SET ISC_API_URL=http://commerce.local.com && SET ISC_ENABLE_CLUSTER=true && SET ISC_FRONT_END_PORT=3000 && ECHO Starting... && node -r source-map-support/register startProduction.js",
    "start:styleguide": "npm start --prefix ./modules/mobius-styleguide",
    "stylelint": "stylelint \"./modules/**/*.tsx\"",
    "test": "cd modules && jest",
    "test:mobius": "cd modules/mobius && jest",
    "test:spire-linter": "cd modules/spire-linter && jest"
  },
